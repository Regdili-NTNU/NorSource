[ SYNSEM.LOCAL.CAT.HEAD verb-all & #head & [ VOICE active, IMPERATIVE -, MOD <>],
  SYNSEM.LOCAL.CAT.HEAD.MAL #mal & [MALDROPPY cop-droppy],
  SYNSEM.LOCAL.CAT.VAL.SUBJ <[LOCAL.CONT.HOOK.INDEX #varg]>,
