  SYNSEM.LOCAL.CONT.HOOK.XARG #varg,
  C-CONT [ RELS <! [PRED "_ha-perfective_rel",
		    ARG0 #eindex,
		    ARG1 #arg1 ] !> ],
  ARGS < [ SYNSEM.LOCAL.CAT.HEAD #head & [MAL #mal],
	   SYNSEM.LOCAL.CAT.VAL.SUBJ < synsem >,
	   SYNSEM.LOCAL.CAT.VAL.COMPS <>,
